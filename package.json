    "test": "pytest"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "nuxt": "^4.1.2",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "sass": "^1.93.0",
    "sass-loader": "^16.0.5"
  }
