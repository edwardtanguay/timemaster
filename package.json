{
  "name": "datapod-python-nuxt",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev -p 3064 -o",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
